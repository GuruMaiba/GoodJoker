@model ICollection<GoodJoker.Models.AdminAjaxList>

@if (Model.Count > 0)
{
    foreach (var item in Model)
    {
        if (item.TopHeader != null && item.TopHeader != "")
        {
            <div class="item" itId="@item.Id">
                <div class="topHeader">@item.TopHeader</div>
                <div class="itTitle"><span class="name">@item.Name</span> @item.Addition</div>
            </div>
        }
        else
        {
            <div class="item singleLine" itId="@item.Id">
                @item.Name @item.Addition
            </div>
        }
    }
}
else
{
    <div class="item singleLine" itId="0">
        Ничего не найдено
    </div>
}
